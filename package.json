{
  "name": "clean-architecture-api-base",
  "version": "1.0.0",
  "author": "Daniel Melo Alencar",
  "keywords": [
    "clean",
    "architecture",
    "api",
    "base",
    "typescript",
    "nodejs",
    "express",
    "fastify",
    "prisma",
    "jest",
    "axios",
    "bcrypt",
    "dotenv"
  ],
  "license": "ISC",
  "description": "",
  "main": "main.js",
  "type": "commonjs",
  "engines": {
    "node": "21.5.0"
  },
  "scripts": {
    "dist": "tsup src",
    "start:dev": "tsx src/main.ts",
    "start:watch": "tsx watch src/main.ts",
    "start:dist": "npm run dist && node dist/main.js",
    "test": "jest --runInBand",
    "prepare": "husky && husky install",
    "check": "eslint 'src/**/*.ts ",
    "check:fix": "eslint . --fix",
    "prettier": "prettier --write .",
    "commit": "git add . && git commit -m 'any'"
  },
  "dependencies": {
    "@fastify/cors": "^10.0.1",
    "@prisma/client": "^5.21.1",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "fastify": "^5.0.0",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/js": "^9.13.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/supertest": "^6.0.2",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "cz-ptbr-changelog": "^1.0.1",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "globals": "^15.11.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "prisma": "^5.21.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.5",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.11.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-ptbr-changelog"
    }
  }
}
